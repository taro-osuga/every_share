$('#photos').html("<%= j render 'photos_list' %>")
<% if @items.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@items.active && !@items.price.blank? && !@items.item_name.blank? && !@items.photos.blank? && !@items.address.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>